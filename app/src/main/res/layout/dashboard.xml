<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bribg">

    <ImageView
        android:id="@+id/logoImageView"
        android:layout_width="150dp"
        android:layout_height="90dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/logo1"
        android:layout_marginTop="25dp"/>

    <ImageButton
        android:id="@+id/cttnkeuangan"
        android:layout_below="@id/logoImageView"
        android:layout_width="76dp"
        android:layout_height="46dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="86dp"
        android:background="@drawable/cttn" />
    <TextView
        android:id="@+id/text1"
        android:layout_below="@id/cttnkeuangan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cacatan Keuangan"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="20dp"/>
    <ImageButton
        android:id="@+id/dmptdigital"
        android:layout_below="@id/logoImageView"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@drawable/dompet"
        android:layout_marginTop="70dp"
        android:layout_marginLeft="157dp"/>

    <TextView
        android:id="@+id/text2"
        android:layout_below="@id/dmptdigital"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Setor tunai"
        android:layout_marginTop="-1dp"
        android:layout_marginLeft="160dp"/>
    <ImageButton
        android:id="@+id/transfer"
        android:layout_below="@id/logoImageView"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/transfer"
        android:layout_marginTop="85dp"
        android:layout_marginLeft="270dp"/>
    <TextView
        android:id="@+id/text3"
        android:layout_below="@id/transfer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tarik Tunai"
        android:layout_marginTop="6dp"
        android:layout_marginLeft="260dp"/>

    <ImageButton
        android:id="@+id/pulsa"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="270dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/logo_pulsa"
        android:layout_below="@+id/text1"/>
    <TextView
        android:id="@+id/text4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pulsa/data"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="260dp"
        android:layout_below="@+id/pulsa"/>

    <ImageButton
        android:id="@+id/briva"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_below="@+id/text2"
        android:layout_marginLeft="65dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/briva" />
    <TextView
        android:id="@+id/text5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="BRIVA"
        android:layout_marginTop="9dp"
        android:layout_marginLeft="65dp"
        android:layout_below="@+id/briva"/>
    <ImageButton
        android:id="@+id/listrik"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_below="@+id/text3"
        android:layout_marginLeft="164dp"
        android:layout_marginTop="15dp"
        android:background="@drawable/logolistrik" />
    <TextView
        android:id="@+id/text6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Listrik"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="170dp"
        android:layout_below="@+id/listrik"/>

    <TextView
        android:id="@+id/text7"
        android:layout_width="131dp"
        android:layout_height="48dp"
        android:layout_below="@+id/text6"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="25dp"
        android:text="Dompet Digital" />

    <ImageButton
        android:id="@+id/gopay"
        android:layout_width="120dp"
        android:layout_height="55dp"
        android:layout_below="@+id/text5"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/gopay"/>

    <TextView
        android:id="@+id/text8"
        android:layout_width="131dp"
        android:layout_height="48dp"
        android:layout_below="@+id/gopay"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:text="Catatan Keuangan" />
    <TextView
        android:id="@+id/textlain"
        android:layout_below="@id/pulsa"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="280dp"
        android:layout_marginTop="90dp"
        android:text="Lainnya" />
    <Button
        android:id="@+id/pengeluaran"
        android:layout_below="@id/textlain"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="95dp"
        android:layout_marginLeft="210dp"
        android:background="@drawable/ic_pengeluaran"/>

    <TextView
        android:id="@+id/text9"
        android:layout_width="131dp"
        android:layout_height="48dp"
        android:layout_below="@id/pengeluaran"
        android:layout_marginLeft="245dp"
        android:layout_marginTop="-20dp"
        android:text="Pengeluaran" />

    <TextView
        android:id="@+id/text10"
        android:layout_width="131dp"
        android:layout_height="48dp"
        android:layout_below="@+id/gopay"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="80dp"
        android:text="Pemasukan" />

    <ImageButton
        android:id="@+id/pemasukan"
        android:layout_width="40dp"
        android:layout_height="25dp"
        android:layout_below="@+id/text5"
        android:layout_marginTop="190dp"
        android:layout_marginLeft="40dp"
        android:background="@drawable/pemasukaan"/>

    <Button
        android:id="@+id/home"
        android:layout_below="@id/textlain"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="205dp"
        android:layout_marginLeft="165dp"
        android:background="@drawable/ic_home"/>

    <Button
        android:id="@+id/opsi"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="370dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/ic_opsi_24"/>

    <Button
        android:id="@+id/back"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/ic_back"/>
</RelativeLayout>